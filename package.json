{
    "name": "od",
    "version": "1.1.6",
    "description": "Oh dear, another date library",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "module": "lib/index.esm.js",
    "scripts": {
        "prepublishOnly": "npm run clean && npm run compile",
        "compile": "rollup -c",
        "clean": "rimraf --no-glob lib",
        "ava": "ava -s -v",
        "lint": "eslint --ext ts .",
        "test": "nyc ava",
        "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
        "push-with-tags": "git push origin master && git push origin --tags",
        "push-and-publish": "npm run push-with-tags && npm publish"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/strong-roots-capital/od.git"
    },
    "keywords": [
        "date"
    ],
    "author": "Eric Crosson <eric.s.crosson@utexas.edu> (https://github.com/strong-roots-capital)",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/strong-roots-capital/od/issues"
    },
    "homepage": "https://github.com/strong-roots-capital/od",
    "devDependencies": {
        "@rollup/plugin-typescript": "^3.0.0",
        "@types/node": "^13.7.4",
        "@typescript-eslint/eslint-plugin": "^2.21.0",
        "@typescript-eslint/parser": "^2.21.0",
        "@wessberg/rollup-plugin-ts": "^1.2.17",
        "ava": "^3.4.0",
        "codecov": "^3.6.5",
        "eslint": "^6.8.0",
        "nyc": "^15.0.0",
        "rimraf": "^3.0.2",
        "rollup": "^1.31.1",
        "tldrlegal": "^1.0.11",
        "ts-node": "^8.6.2",
        "tslib": "^1.11.0",
        "typescript": "^3.8.2"
    },
    "files": [
        "lib/"
    ],
    "ava": {
        "files": [
            "!dist/**/*.d.ts",
            "test/**/test-*.ts"
        ],
        "extensions": [
            "ts"
        ],
        "require": [
            "ts-node/register"
        ]
    },
    "nyc": {
        "include": [
            "src/*.ts"
        ],
        "extension": [
            ".ts"
        ],
        "require": [
            "ts-node/register"
        ]
    },
    "publishConfig": {
        "access": "public"
    }
}
